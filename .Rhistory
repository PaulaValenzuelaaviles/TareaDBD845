pheno_e$Treatment<-as.factor(pheno_e$Treatment)
pheno_e$Program<-as.factor(pheno_e$Program)
pheno_e$Sampling<-as.factor(pheno_e$Sampling)
pheno_e$Sex<-as.factor(pheno_e$Sex)
pheno_e$Tank<-as.factor(pheno_e$Tank)
